{
  "expo": {
    "name": "Easy Shop",
    "slug": "easy-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adebiyiart.easyshop"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7777e318-693b-469f-8fcb-f6ae081b0430"
      }
    }
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./local/assets/notification-icon.png",
        "color": "#ffffff"
      }
    ]
  ]
}
